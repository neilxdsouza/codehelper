Grammar

    0 $accept: prog $end

    1 prog: statement_list

    2 statement_list: statement
    3               | statement_list statement

    4 statement: CREATE TABLE NAME '(' decl_comma_list ')' ';'

    5 data_type: INT32_T
    6          | FLOAT_T
    7          | BIGINT_T
    8          | DOUBLE_T
    9          | VARCHAR_T
   10          | NVARCHAR_T
   11          | NCHAR_T
   12          | NTEXT_T
   13          | TEXT_T
   14          | BIT_T
   15          | DATETIME_T
   16          | TINYINT_T
   17          | IMAGE_T

   18 decl_comma_list: var_decl_with_or_wo_options
   19                | decl_comma_list ',' var_decl_with_or_wo_options

   20 var_decl_with_or_wo_options: var_decl ':' options_list

   21 options_list: options
   22             | options_list options

   23 options: REFERENCES NAME '(' NAME ')'
   24        | REFERENCES MANY NAME '(' NAME ')'
   25        | DBNULL
   26        | NOT DBNULL
   27        | UNIQUE
   28        | VALIDATOR_REQ_FIELD
   29        | VALIDATOR_RE_INTEGER
   30        | VALIDATOR_RE_FLOAT
   31        | VALIDATOR_RE_ALPHANUM
   32        | VALIDATOR_RE_ALPHANUMWSP
   33        | SEARCH_KEY

   34 var_decl: NAME data_type
   35         | NAME data_type '(' INUMBER ')'


